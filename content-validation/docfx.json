{
  "build": {
    "content": [
      {
        "group": "tfs",
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "group": "tfs",
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/content-validation/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "author": "test"
    },
    "fileMetadata": 
    {
    },
    "template": [
      "docs.html"
    ],
    "dest": "content-validation",
    "markdownEngineName": "markdig",
  }
}
