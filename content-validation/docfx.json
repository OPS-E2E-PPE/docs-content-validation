{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/content-validation/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "author": "test"
    },
    "fileMetadata": 
    {
      "ms.author":{
        "fileMetadata/*.md": "abc"
      },
      "monikerRange":{
        "Headings/*.md": "tfs-2015 || tfs-2017 || azure-devops || >= tfs-2017 <= tfs-2018"
      }
    },
    "template": [],
    "dest": "content-validation",
    "markdownEngineName": "markdig"
  }
}
